function getUrlParams(e){var t,r,a,n=document.currentScript&&document.currentScript.src?document.currentScript.src:null,i={};if(n&&(t=n.split("?")).length>1)for(t=t[1].split("&"),a=0;a<t.length;a++)(r=t[a].split("=")).length>1&&2==r.length&&(i[r[0]]=decodeURIComponent(r[1].replace(/\+/g," ")));return i}function writePixel(e){var t=document.createElement("img");t.style.display="none",t.src=e;var r=document.getElementsByTagName("body")[0]||document.body;r?r.appendChild(t):window.addEventListener("DOMContentLoaded",function(){document.body.appendChild(t)})}function writeSmartTracker(e,t){if(e&&t){var r=t.split(";");for(var a in r){writePixel("https://ww1289.smartadserver.com/h/tp?advid="+e+"&trcid="+r[a]+"&val=[value]&transid=[transactionid]&ref=[reference]&uid=[sas_uid]&tmstp="+Math.round(1e10*Math.random()))}}}function writeMediaSmartTracker(e){if(e){var t=e.split(";");try{var r=window.location.href,a=-1!=(i=r.indexOf("msck"))?r.substring(i):"MSCLICKID",n=a.indexOf("&");for(var i in-1!=n&&(a=a.substring(0,n)),t){writePixel("https://ads.mediasmart.es/m/open?id=mozoo-"+t[i].split(",event")[0]+"&ms_event_num="+(t[i].split(",event").length>1?t[i].split(",event")[1]:"1")+"&udid="+encodeURIComponent(a)+"&burst="+(new Date).getTime())}}catch(e){}}}var urlParams=getUrlParams();urlParams.a&&(urlParams.a.indexOf("s")>-1&&writeSmartTracker(urlParams.aid,urlParams.tid),urlParams.a.indexOf("m")>-1&&writeMediaSmartTracker(urlParams.cid));