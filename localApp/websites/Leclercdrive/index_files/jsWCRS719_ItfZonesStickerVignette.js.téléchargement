(function(){this.jsWCRS719_ItfZonesStickerVignette=Utilitaires.Class.extend({lstStickersProduit:null,sClasseCSSstickerInvalide:"StickerInvalide",sClasseCSSsansCorner:"divWCRS310_SansCorner",fCorner:!1,fStickerNutri:!1,iLimitationSiStickerNutri:3,init:function(n,t,i){this.lstStickersProduit=n;this.lstStickers=t;this.fGainsBRD=i;this.fCorner=_.any(n,{iIdZone:10101});this.fCorner==!1&&(this.fCorner=_.any(n,{iIdZone:10201}));this.fStickerNutri=_.any(n,{iIdZone:10107},{iIdZone:10207},{iIdZone:10307});this.fStickerNutri&&this.fCorner&&this.iLimitationSiStickerNutri--},StickersZone:function(n){var o=this.lstStickersProduit,u=this.lstStickers,e=0,t=_.where(o,{iIdZone:n}),f,i,r;for(this.fStickerNutri&&n===10102&&t.length>this.iLimitationSiStickerNutri&&(t=_.first(t,this.iLimitationSiStickerNutri)),n===10104&&t.length>1&&(t=_.first(t,1)),f=[],i=0;i<t.length;i++)for(r=0;r<u.length;r++)t[i].iIdSticker==u[r].iIdSticker&&(f[e]={StickersProduit:t[i],Sticker:u[r]},e++);return f},RetaillerSticker:function(n,t){return n.sUrlCheminComplet&&(n.sUrlCheminComplet=n.sUrlCheminComplet.replace("*MINDIM*",t)),n},ClasseCssPictoBRD:function(){if(this.fGainsBRD)return this.sClasseCSSstickerInvalide},ClasseCssavecCorner:function(){if(this.fCorner==!1)return this.sClasseCSSsansCorner},TypeTooltip:function(n){return n.sTypeSticker=="JEU"?'data-tooltip2="StickerTexte"':n.sTypeSticker=="DESTOCKAGE"?'data-tooltip2="StickerTexte"':n.sTypeSticker=="COUP_DE_COEUR"?'data-tooltip2="StickerTexte"':void 0}})}).call(this.Interfaces=this.Interfaces||{__namespace:!0},window.jQuery);typeof Sys!="undefined"&&Sys.Application.notifyScriptLoaded();