var WPAD330=WPAD330||{};$(document).ready(function(){WPAD330.VerifierNavigateur();$("#lnkWPAD330_Fermer").on("click",function(n){n.preventDefault();n.stopPropagation();WCTD204.SetCookie({cle:"VerNav",value:null,path:"/",domain:null});$(document.getElementById("divWPAD330_Bandeau")).slideUp("fast",function(){$(document.getElementById("divWPAD330_Bandeau")).addClass("masquer")})})});WPAD330={VerifierNavigateur:function(){var t=!1,n;try{if(n=Array(),n=WCTD204.IdentifierNavigateur(undefined,1),WCTD204.GetCookie("VerNav")!=null||WCTD204.EstMobile()||n===null)t=!1;else switch(n.browser.toLowerCase()){case"msie":n.version<11&&(t=!0);break;case"firefox":n.version<40&&(t=!0);break;case"chrome":n.version<45&&(t=!0);break;case"safari":n.version<9&&(t=!0)}}catch(i){}t&&$(document.getElementById("divWPAD330_Bandeau")).removeClass("masquer")}};
var WPAD376=WPAD376||{};$(function(){$("#btnWPAD376_Valider").on("click",function(n){WPAD376.stopEvent(n);WPAD376.Valider()})});WPAD376={Valider:function(){if($(".divWPAD376_MsgSucces").addClass("masquer"),$(".divWPAD376_MsgEchec").addClass("masquer"),WCTD601.Validation.Valider("FormulaireOuverturePrivee")){var n={sCodePostal:$("[id$=_txtWPAD376_CodePostal]").val(),sEmail:$("[id$=_txtWPAD376_Email]").val()};WPAD376.ValiderSucces()}},ValiderEchec:function(){$(".divWPAD376_MsgSucces").addClass("masquer");$(".divWPAD376_MsgEchec").removeClass("masquer")},ValiderSucces:function(){$(".divWPAD376_MsgSucces").removeClass("masquer");$(".divWPAD376_MsgEchec").addClass("masquer")},OuvrirPopinConnexionPrivee:function(n){WCTD201.Class.PopinManager.OuvrirPopin({sNomPopin:"WCLD077_OuverturePopinConnexionPrivee",data:{sNoPointLivraison:n.data("pl"),sIdUnivers:n.data("univers")}})},OuvrirPreinscription:function(n){Utl.Loader.afficher();window.location=Utilitaires.Ressources.ascWPAD376_OuverturePrivee.PARAM_URL_PREINSCRIPTION.replace("{0}",n.data("pl"))},AfficherOuverturePrivee:function(n,t,i){$("#divWPAD042_OuverturePrivee").show();$("#divWPAD042_OuverturePrivee .txtWPAD376_CodePostal").val(n);$("#divWPAD042_OuverturePrivee .aWPAD376_OuverturePrive").data("univers",t);$("#divWPAD042_OuverturePrivee .aWPAD376_OuverturePrive").data("pl",i);$("#divWPAD042_OuverturePrivee .aWPAD376_Preinscription").data("pl",i);$("#divWPAD042_OuverturePrivee .txtWPAD376_Email").focus()},MasquerOuverturePrivee:function(){$("#divWPAD042_OuverturePrivee").hide()},stopEvent:function(n){n.preventDefault();n.stopPropagation()}};

if(typeof(Sys)!=='undefined')Sys.Application.notifyScriptLoaded();
