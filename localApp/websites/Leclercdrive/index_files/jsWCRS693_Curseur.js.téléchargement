(function(){this.Curseur=function(n){var t=1,i=1,r=n,u=new ListesProduits_Trade.Place,f={at:function(n){u=n}},e=function(n){return n.verifyCurseur?n.verifyCurseur(t,i,r):!0};this.move=function(n){var e=i+n,u;return t+=Math.floor(e/(r+1)),u=e%r,i=u!=0?u:r,f};this.moveNextLine=function(){return t=t+1,i=1,f};this.verifyPolicy=function(n){var t=!0;return n?(u.verify&&(t=t&&u.verify(n)),t&&e(n)):!0};this.incrementCounter=function(n,f){u.incrementCounter&&u.incrementCounter(n,f);f.incrementCounterCurseur&&f.incrementCounterCurseur(n,t,i,r)};this.renderCss=function(n){return n+" colonne"+i+" ligne"+t}}}).call(this.ListesProduits_Trade=this.ListesProduits_Trade||{__namespace:!0},window.jQuery);typeof Sys!="undefined"&&Sys.Application.notifyScriptLoaded();